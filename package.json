{
  "name": "ewally-test",
  "version": "1.0.0",
  "main": "server.js",
  "license": "MIT",
  "scripts": {
    "dev": "ts-node-dev --exit-child src/server.ts",
    "build": "tsc",
    "clean": "rimraf ./build",
    "start": "node build/server.js",
    "jest-clear-cache": "jest --clearCache",
    "test-middleware": "jest --watch --detectOpenHandles src/modules/bills/BillMiddleware.spec.ts",
    "test-service": "jest --watch --detectOpenHandles src/modules/bills/BillService.spec.ts",
    "test-validators": "jest --watch --detectOpenHandles src/modules/bills/validators/*.spec.ts",
    "test-concessionarie-validator": "jest --watch --detectOpenHandles src/modules/bills/validators/ConcessionarieBillValidator.spec.ts",
    "test-bank-validator": "jest --watch --detectOpenHandles src/modules/bills/validators/BankBillValidator.spec.ts",
    "test-utils": "jest --watch --detectOpenHandles src/modules/bills/utils.spec.ts",
    "integration-test": "jest --watch --detectOpenHandles src/app.integration.spec.ts",
    "unit-test-bill": "jest --watch --detectOpenHandles src/modules/bills/*.spec.ts",
    "all-tests": "jest --detectOpenHandles"
  },
  "dependencies": {
    "dotenv": "^10.0.0",
    "express": "^4.17.1",
    "morgan": "^1.10.0"
  },
  "devDependencies": {
    "@types/express": "^4.17.12",
    "@types/jest": "^26.0.24",
    "@types/morgan": "^1.9.3",
    "@types/supertest": "^2.0.11",
    "jest": "^27.0.6",
    "rimraf": "^3.0.2",
    "supertest": "^6.1.3",
    "ts-jest": "^27.0.3",
    "ts-node-dev": "^2.0.0",
    "typescript": "^4.3.5"
  }
}
